.container.user-page
  %header.user-nav
    .media
      .media-left
        =link_to '#' do
          = image_tag '', class: 'media-object', alt: '64x64', data: { holder:{ rendered: true }, src: 'holder.js/64x64', style: 'width: 64px; height: 64px;'}
      .media-body
        %h4#top-aligned-media.media-heading
          Top aligned media
          %span.label.label-default.project-number
            29
            %i Projects
        .proto-user
          %span.degree
            = @user.member
        .row
          .col-md-6
            %h4 Profile
            %p #{@user.profile}
          .col-md-6
            %h4 Works
            %p #{@user.works}
.container.proto-list
  .row
    =render @prototype

.text-center
  %ul.pagination
    %li.disabled
      =link_to '#',aria: {label: "Previous"} do
        %span{area: {hidden:true}} «
    %li.active
      =link_to '/' do
        %span.sr-only (current)
    %li
      =link_to "2",'#'
    %li
      =link_to "3",'#'
    %li
      =link_to "4",'#'
    %li
      =link_to "5",'#'
    %li
      =link_to '#',aria: {label: "Next"} do
        %span{area: {hidden:true}} »



